def main():
    num = int(input("inserisci un numero:"))
    if num%2 == 0:
        print("il numero si puo dividere per 2")
    elif num%3 == 0:
        print("il numero si puo dividere per 3")
    elif num%5 == 0:
        print("il numero si puo dividere per 5")
    else:
        print("il numero non si puÃ² dividere ne per 2 ne per 3 ne per 5")
if __name__ == "__main__":
    main()